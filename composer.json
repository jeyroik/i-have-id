{
    "name": "jeyroik/i-have-id",
    "description": "ID attribute traits",
    "require": {
        "jeyroik/i-have-attributes": "0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "0.*"
    },
    "autoload": {
        "psr-4": {
            "jeyroik\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox --coverage-clover=coverage.xml --whitelist=src tests",
        "stan": "vendor/bin/phpstan analyse -l 5 src"
    }
}